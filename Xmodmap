! Alt_L becomes Super_L
! Caps_Lock becomes Control_L
! Control_L becomes Alt_L
! Super_L becomes Caps_Lock

remove Control = Control_L
remove Lock = Caps_Lock
remove mod1 = Alt_L
remove mod4 = Super_L

keysym Alt_L = Super_L
keysym Caps_Lock = Control_L
keysym Control_L = Alt_L
keysym Super_L = Caps_Lock

add Control = Control_L
add Lock = Caps_Lock
add mod1 = Alt_L
add mod4 = Super_L
